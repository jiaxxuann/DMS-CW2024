<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER"
      spacing="20"
      xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.demo.ui.MainMenuController"
      prefHeight="400.0" prefWidth="600.0"
      style="-fx-background-image: url('/com/example/demo/images/background5.jpg');
             -fx-background-size: cover;
             -fx-background-position: center;">
   <children>
      <Button fx:id="startButton" mnemonicParsing="false" text="Start Game" />
      <Button fx:id="settingsButton" mnemonicParsing="false" text="Settings" />
      <Button fx:id="quitButton" mnemonicParsing="false" text="Quit" />
   </children>
</VBox>
